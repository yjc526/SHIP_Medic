    current_searching_position();
    moveSlider(1);
    center = map.getCenter();
    temp_coordinates = map.getCenter();
    setTimeout(list_open, 602);
  }

  function list_open() {
      list_open = 1;
      moveSlider(0);
      $("#animation_canvas").css("pointer-events", "visiblepainted");
      $("#control_button_on").css("display", "none");
      $("#control_button_off").css("display", "inline");
      $("#search_here_text").css("width", "50%");
      $("#search_here_panel").css("padding-right", "30px");
      $("input[name='list_open']").val("1");
      setTimeout(marker_makes_map_small, 600);
      setTimeout(set_center, 601);
      if (ie9) { setTimeout($("#animation_canvas").show(), 0); }
  }

  show_map();
