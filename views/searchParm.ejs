  /********** Get values of Geocoding reverse **********/
  var geocoder = new google.maps.Geocoder();

  function code_lat_lng() {
    var lat = 37.516034
    var lng = 127.022124
    var latlng = new google.maps.LatLng(lat, lng);
    geocoder.geocode({'location': latlng}, function(results, status) {
      if (status == google.maps.GeocoderStatus.OK) {
        if (results[1]) {
          console.log(results[1].formatted_address);
          addr = results[1].formatted_address;
          addr_length = addr.split(" ").length;
          sido = addr.split(" ")[addr_length - 3];
          gugun = addr.split(" ")[addr_length - 2];
          myundong = addr.split(" ")[addr_length - 1];
          $("#navigator #area .title").empty();
          $("#navigator #area .title").append(gugun + " " + myundong);
        } else {
          $("#navigator #area .title").empty();
          $("#navigator #area .title").append("결과없음");
          console.log('No results found');
        }
      } else {
        $("#navigator #area .title").empty();
        $("#navigator #area .title").append("정보없음");
        console.log('Geocoder failed due to: ' + status);
      }
    });
  }

