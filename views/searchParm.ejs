  /********** Separate markers **********/
  function separate_markers(marker, array_num) {
    google.maps.event.addListener(marker, 'click', function() {
      var t = 0;
      for(i = 0; i < marker_array.length; i++) {
        if(marker.getPosition().lat() == marker_array[i].lat() && marker.getPosition().lng() == marker_array[i].lng()) {
          save_to_separate.push(i);
          t = t + 1;
        }
      }
      if(t > 1) {
        show_markers_info(save_to_separate);
      }
      else {
        scrolling(marker, array_num);
      }
      save_to_separate.length = 0;
    });
  }

